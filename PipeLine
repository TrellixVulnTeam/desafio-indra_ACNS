#!groovy 

pipeline  { 
  agent any
  stages {
    stage('Preparation') { 
      steps {
        git 'https://github.com/daniel19942015/desafio-indra.git' 
        
   
      }
  }
    stage('Build') {
      steps {
      
        bat  '''
        
        env//Scripts//activate
        python -m Pyautomators
        
        
        '''
       
     
      }
    } 
    stage('Results')  { 
      steps {
        echo 'Results' 
      }        
    } 
  }
}
